{
  "name": "rx-ui-suite",
  "version": "0.0.0-pre1",
  "description": "",
  "scripts": {
    "in:shared": "yarn --cwd packages/rx-ui-shared",
    "in:component": "yarn --cwd packages/rx-component",
    "in:dom": "yarn --cwd packages/rx-dom",
    "in:tools": "yarn --cwd packages/tools",
    "install:in:_react": "yarn --cwd __packages/react install",
    "install:in:_preact": "npm --prefix __packages/preact install",
    "install:in:_inferno": "npm --prefix __packages/inferno install",
    "shared:in:component": "yarn in:component add file:../rx-ui-shared",
    "shared:in:dom": "yarn in:dom add file:../rx-ui-shared",
    "shared:in:tools": "yarn in:tools add file:../rx-ui-shared",
    "shared:everywhere": "yarn shared:in:component && yarn shared:in:dom && yarn shared:in:tools",
    "babel": "./node_modules/.bin/babel --extensions \".tsx\"",
    "link:packages": "yarn add link:./packages/rx-ui-shared link:./packages/rx-dom link:./packages/rx-component link:./packages/rx-ui-tools",
    "file:packages": "yarn add file:./packages/rx-ui-shared file:./packages/rx-dom file:./packages/rx-component link:./packages/rx-ui-tools",
    "remove:packages": "yarn remove rx-ui-shared rx-dom rx-component rx-ui-tools",
    "test": "jest"
  },
  "private": true,
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.10.2",
    "@rx-ui/core": "file:./packages/core",
    "rxjs": "^7.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-rx-ui-jsx": "file:./plugins/babel-plugin-transform-rx-ui-jsx",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": "git+https://github.com/adamf92/rx-ui-suite.git#master"
}
